# Firebase Functions Environment Variables Template
# Copy this file to .env and fill in your values

# Google Calendar OAuth
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
GOOGLE_REDIRECT_URI=https://your-project.cloudfunctions.net/api/calendars/google/callback

# Microsoft Outlook OAuth
MS_GRAPH_CLIENT_ID=your_microsoft_client_id_here
MS_GRAPH_CLIENT_SECRET=your_microsoft_client_secret_here
MS_REDIRECT_URI=https://your-project.cloudfunctions.net/api/calendars/outlook/callback

# Stripe (if using)
STRIPE_SECRET_KEY=your_stripe_secret_key_here
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret_here

# OpenAI (for AI Assistant)
OPENAI_API_KEY=your_openai_api_key_here
